<div class="container">

  <div class="page-title">
    <h1>Categories</h1>
  </div>

  <div class="row-wrap-list">
    <% if (!categoryList.length) { %>
      <p>No results </p>
    <% } else { 
      categoryList.forEach(category => { %>
        <div class="box">
          <area class="image-box" href="<%- showCategoryPath(category) %>"/>

          <div class="in-box-footer">
            <a href="<%- showCategoryPath(category) %>"><%- category.name %></a>
          </div>
        </div>
      <% }) %>
    <% } %>
  </div>

</div> 