<div class="container">

    <div class="page-title">
      <h1>Reservations</h1>
    </div>
    
    <div class="row-wrap-list">
      <% if (!reservationsList.length) { %>
        <p>No results </p>
      <% } else { 
        reservationsList.forEach(reservation => { %>
          <div class="box">
            <area class="image-box" href="<%- showReservationPath(reservation) %>"/>

            <div class="in-box-footer">
              <a href="<%- showReservationPath(reservation) %>"><%- reservation.id %></a>
            </div>
          </div>
        <% }) %>
      <% } %>
    </div>

  
  </div> 